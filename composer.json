{
    "name": "lukasmundt/lumosbaseapplication",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "elibyy/tcpdf-laravel": "^10.0",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.3",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "league/csv": "9.0",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.5",
        "spatie/laravel-navigation": "^1.2",
        "spatie/laravel-permission": "^5.10",
        "spatie/simple-excel": "^3.2",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.21",
        "laravel/dusk": "^7.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Lukasmundt\\Akquise\\": "modules/lukasmundt/akquise/src/",
            "Lukasmundt\\ProjectCI\\": "modules/lukasmundt/projectci/src/",
            "Lukasmundt\\Finances\\": "modules/lukasmundt/finances/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "installer-paths": {
            "modules/{$name}": [
                "type:lumos-module"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
